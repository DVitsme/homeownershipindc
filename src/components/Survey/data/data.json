      "type": "radio",
      "options": [
        "Less than $25,000",
        "$25,000 to $50,000",
        "$50,000 to $75,000",
        "$75,000 to $100,000",
        "$100,000 to $150,000",
        "$150,000 to $200,000",
        "More than $200,000"
      ]
    },

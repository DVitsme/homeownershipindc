{
  "sectionTitle": [
    {
      "jsonTitle": "aboutYou",
      "displayTitle": "About You",
      "subTitle": null
    },
    {
      "jsonTitle": "yourHousehold",
      "displayTitle": "Your Household",
      "subTitle": "Income and wealth"
    },
    {
      "jsonTitle": "yourNeighborhood",
      "displayTitle": "Your Neighborhood",
